{
	"name":"ArDrone2Module",
	"type":"background",
	"category":"Robot",
	"description":"Provides a controller for the Parrot AR.Drone 2.0",
	"default_middleware":"android",
	"supported_middleware": ["android"],
	"supported_devices": ["android"],
	"enable":"true",
	"git":"https://github.com/eggerdo/aim_modules.git",
	"android": {
		"package":"org.dobots.ardrone2",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.ardrone2"
	},
	"ports": [
		{
			"name":"cmd",
			"dir":"in",
			"type":"string",
			"middleware":"android"
		},
		{
			"name":"video",
			"dir":"out",
			"type":"string",
			"middleware":"android"
		}
	]
}
