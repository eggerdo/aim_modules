{
	"name":"CameraModule",
	"type":"background",
	"category":"Image processing",
	"description":"Provides Android Camera as video output. Video frames are sent as jpg images. (as raw jpg or base64 encoded string)",
	"default_middleware":"android",
	"supported_middleware": ["android"],
	"supported_devices": ["android"],
	"enable":"true",
	"git":"https://github.com/eggerdo/aim_modules.git",
	"android": {
		"package":"org.dobots.cameramodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.cameramodule"
	},
	"ports": [
		{
			"name":"cmd",
			"dir":"in",
			"type":"string",
			"middleware":"android"
		},
		{
			"name":"video_base64",
			"dir":"out",
			"type":"string",
			"middleware":"android"
		},
		{
			"name":"video_raw",
			"dir":"out",
			"type":"intarray",
			"middleware":"android"
		}
	]
}
