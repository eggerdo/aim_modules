{
	"name":"RobotControlModule",
	"type":"UI",
	"category":"Robot",
	"description":"Provides a user interface to control robots",
	"default_middleware":"android",
	"supported_middleware": ["android"],
	"supported_devices": ["android"],
	"enable":"true",
	"git":"https://github.com/eggerdo/aim_modules.git",
	"android": {
		"package":"org.dobots.robotcontrol",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.robotcontrol"
	},
	"ports": [
		{
			"name":"cmd",
			"dir":"out",
			"type":"string",
			"middleware":"android"
		},
		{
			"name":"video",
			"dir":"in",
			"type":"string",
			"middleware":"android"
		}
	]
}
